<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - EBookLibrary.Presentation</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <link rel="stylesheet" href="~/css/newStyle.css" />
    <link rel="stylesheet" href="~/font-awesome/css/all.min.css" />
    <link href="~/css/error.css" rel="stylesheet" />
    <link href="~/css/registration.css" rel="stylesheet" />
</head>
<body>
    
    <div class="container-fluid p-0 mb-3 bg-white">
        <main role="main" class="pb-3 mb-3">
            @RenderBody()
        </main>
    </div>


    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/font-awesome/js/all.min.js" type="text/javascript" asp-append-version="false"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>


